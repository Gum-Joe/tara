    "detect-indent": "^5.0.0",
    "electron": "^1.7.9",
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "^2.2.0",
    "env-paths": "^1.0.0",
    "external-editor": "^2.0.5",
