{
  "name": "tara",
  "productName": "tara",
  "version": "0.1.0",
  "description": "An electron based file explorer.",
  "main": "src/renderer/index.js",
  "scripts": {
    "start": "./node_modules/.bin/electron . --color -v",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "eslint src"
  },
  "keywords": [],
  "author": "Kishan",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "zip",
          "squirrel",
          "appx"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "zip",
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {
        "name": "tara"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "Gum-Joe",
        "name": "tara",
        "prerelease": true
      },
      "windowsStoreConfig": {
        "packageName": "Tara",
        "name": "tara"
      }
    }
  },
  "dependencies": {
