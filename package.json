{
  "name": "tara",
  "productName": "tara",
  "version": "0.1.0",
  "description": "An electron based file explorer.",
  "main": "src/renderer/index.js",
  "scripts": {
    "start": "./node_modules/.bin/electron . --color -v",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "eslint src"
  },
  "keywords": [],
  "author": "Kishan",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "zip",
          "squirrel",
          "appx"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "zip",
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {
        "name": "tara"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "Gum-Joe",
        "name": "tara",
        "prerelease": true
      },
      "windowsStoreConfig": {
        "packageName": "Tara",
        "name": "tara"
      }
    }
  },
  "dependencies": {
    "async": "^2.4.1",
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^3.3.7",
    "chalk": "^1.1.3",
    "detect-indent": "^5.0.0",
    "electron": "^1.6.11",
    "electron-compile": "^6.4.1",
    "electron-devtools-installer": "^2.2.0",
    "env-paths": "^1.0.0",
    "font-awesome": "^4.7.0",
    "global": "^4.3.2",
    "jquery": "^3.2.1",
    "less": "^2.7.2",
    "node-sass": "^4.5.3",
    "npm": "^5.0.3",
    "prop-types": "^15.5.9",
    "react": "^15.5.4",
    "react-addons-create-fragment": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-split-pane": "^0.1.63",
    "redux": "^3.6.0",
