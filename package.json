{
  "name": "tara",
  "productName": "tara",
  "version": "0.1.0",
  "description": "An electron based file explorer.",
  "main": "src/renderer/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "eslint src"
  },
  "keywords": [],
  "author": "Kishan",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "zip",
          "squirrel",
          "appx"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "zip",
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {
        "name": "tara"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "Gum-Joe",
        "name": "tara",
        "prerelease": true
      },
      "windowsStoreConfig": {
        "packageName": "Tara",
        "name": "tara"
      }
    }
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "chalk": "^1.1.3",
    "electron-compile": "^6.4.1",
    "electron-devtools-installer": "^2.2.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "less": "^2.7.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "next",
    "semantic-ui": "^2.2.10",
    "semantic-ui-react": "^0.68.2"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "electron-prebuilt-compile": "1.6.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.0",
    "eslint-plugin-react": "^6.10.3"
  }
}
